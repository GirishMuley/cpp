#include<iostream>
using namespace std;

int main(){
    int a = 10;
    cout<<a<<endl;
    a++;//post incremet operator
    cout<<a<<endl;
    ++a;//pre increment operator
    cout<<a<<endl;
    a--;//post decrement operator
    cout<<a<<endl;
    --a;//pre decrement operator
    cout<<a<<endl;
    int b=a;//asignment oprator
    cout<<b<<endl;
    return 0;
}